<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--命名空间应该是对应接口的包名+接口名 -->
<mapper namespace="com.inspur.cmis.permit.login.mapper.LoginDao">

	<select id="userLogin" parameterType="loginBean" resultType="loginBean" >
		select * from yc_member a where a.cname=#{loginName} and a.men_pass=#{loginPass} and a.isEnable = '正常'
	</select>

</mapper>